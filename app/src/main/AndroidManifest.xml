<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testgage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Testgage">
        <activity android:name=".HistoricActivity"></activity>
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".FrontpageListActivity"
            android:label="@string/title_frontpage_list"
            android:theme="@style/Theme.Testgage.NoActionBar" />
        <activity
            android:name=".FrontpageDetailActivity"
            android:label="@string/title_frontpage_detail"
            android:parentActivityName=".FrontpageListActivity"
            android:theme="@style/Theme.Testgage.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testgage.FrontpageListActivity" />
        </activity>
        <activity android:name=".HRMonitorActivity" />
        <activity android:name=".ProfileRegisterActivity" />
        <activity android:name=".WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HistoricDataActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>